@model YerbaSoft.Web.Games.Clue.DTO.Usuario
@{
    ViewBag.Title = "Administrador de Usuarios";
    Layout = "~/Views/Shared/_Base.cshtml";
}

<div class="row login">
    <div class="col-md-5 col-md-offset-3">
        <div class="panel panel-primary">
            <div class="panel-heading">@Html.Raw(Model.Id == default(Guid) ? "Nuevo Usuario" : "Edición de Usuario")</div>
            <div class="panel-body">
                @Html.Partial("./EditUsuario", Model)
            </div>
        </div>
    </div>
</div>